	<section id="cne-block" style="width:400px; margin-top:150px; margin-left:475px;">
		<div class="block-border"><div class="block-content">
			
			<!--
			IE7 compatibility: if you want to remove the <h1>, 
			add style="zoom:1" to the above .block-content div
			-->
			<div class="block-header">Create CNE</div>
				
			<%= form_for(@cne, html: {class: "form with-margin", name: "cne-form", id: "cne-form"}, :url => {:action => :cne_created}) do |f| %>
			<p class="inline-small-label" style: "margin-bottom:20px;" >
				<%= f.label :circle %>				
				<%= f.collection_select :circle, @circle, :name, :name, include_blank: true, style: "margin-bottom:20px;" %>			
			</p>
			<br>
			<p class="inline-small-label">
				<%= f.label :cne_name %>
				<%= f.text_field :name, id: "cne_name", class: "full-width", value: "", style: "margin-bottom:20px;" %>
			</p>
			<p class="inline-small-label">
				<%= f.label :cne_ID %>
				<%= f.text_field :cne_id, id: "cne_id", class: "full-width", value: "", style: "margin-bottom:20px;" %>
			</p>
			<p class="inline-small-label">
				<%= f.label :location %>
				<%= f.text_field :location, id: "cne_location", class: "full-width", value: "", style: "margin-bottom:20px;" %>
			</p>
			<p class="inline-small-label">
				<%= f.label :incharge_name %>
				<%= f.text_field :incharge_name, id: "cne_incharge_name", class: "full-width", value: "", style: "margin-bottom:20px;" %>
			</p>
			<p class="inline-small-label">
				<%= f.label :incharge_phone %>
				<%= f.text_field :incharge_contact, id: "cne_incharge_contact_no", class: "full-width", value: "", style: "margin-bottom:20px;" %>
			</p>
			<p class="inline-small-label">
				<%= f.label :incharge_email %>
				<%= f.email_field :incharge_email, id: "cne_incharge_email", class: "full-width", value: "", style: "margin-bottom:20px;" %>
			</p>
			<%= f.button :Create, class: "float-right" %>
			<% end %>









